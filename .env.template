# Google Cloud Configuration
REACT_APP_GOOGLE_CLOUD_PROJECT_ID=mia-logistics-manager-2025

# Google Sheets Configuration
REACT_APP_GOOGLE_SHEETS_SPREADSHEET_ID=your_spreadsheet_id_here

# Google Maps Configuration
REACT_APP_GOOGLE_MAPS_API_KEY=your_maps_api_key_here

# Development Configuration
REACT_APP_ENVIRONMENT=development
# Backend API base URL (frontend will call this). Default backend port is 5050.
BACKEND_PORT=5050
FRONTEND_PORT=3000
REACT_APP_API_BASE_URL=http://localhost:${BACKEND_PORT}

# Service Account (Server-side only)
# Note: service-account-key.json should be in /server/ directory
GOOGLE_SERVICE_ACCOUNT_EMAIL=mia-logistics-service@mia-logistics-manager-2025.iam.gserviceaccount.com
