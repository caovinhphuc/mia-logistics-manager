{
  "name": "mia-logistics-manager",
  "version": "1.0.0",
  "description": "MIA Logistics Manager - Hệ thống quản lý vận chuyển chuyên nghiệp",
  "main": "src/index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "start": "react-scripts start",
    "start:unified": "REACT_APP_ENTRY=unified react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "npm test -- --coverage --watchAll=false",
    "test:watch": "npm test -- --watch",
    "test:e2e": "echo 'E2E tests not configured yet'",
    "eject": "react-scripts eject",
    "dev:frontend": "sh -c 'FRONTEND_PORT=${FRONTEND_PORT:-3000} react-scripts start'",
    "dev:backend": "sh -c 'BACKEND_PORT=${BACKEND_PORT:-3100} cd backend && npm start'",
    "build:prod": "npm run build && npm run optimize",
    "optimize": "echo 'Optimization complete'",
    "serve": "sh -c 'PORT=${PORT:-3000} && serve -s build -l $PORT'",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src/ --ext .js,.jsx,.ts,.tsx --fix",
    "lint:check": "eslint src/ --ext .js,.jsx,.ts,.tsx --max-warnings 0",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "pre-commit": "npm run lint:check && npm run format:check && npm run type-check",
    "postinstall": "husky install || true",
    "deploy": "npm run build && npm run deploy:vercel",
    "deploy:netlify": "netlify deploy --prod --dir=build",
    "deploy:vercel": "vercel --prod",
    "deploy:github": "gh-pages -d build",
    "backup:sheets": "node backend/scripts/export-all-sheets.js",
    "restore:sheets": "node backend/scripts/import-sheets.js",
    "verify:migration": "node backend/scripts/verify-migration.js --all",
    "backup:pre-migration": "./scripts/backup-before-migration.sh",
    "health:check": "curl http://localhost:5050/api/health",
    "test:connections": "sh scripts/test-connections.sh || echo 'Connection test script not found'",
    "security:audit": "npm audit && npm audit fix",
    "deps:check": "npx depcheck",
    "deps:update": "npx npm-check-updates -u",
    "analyze": "npm run build && npx webpack-bundle-analyzer build/static/js/*.js"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.7",
    "@mui/material": "^7.3.7",
    "@mui/system": "^7.3.7",
    "@mui/x-date-pickers": "^8.27.0",
    "@sendgrid/mail": "^8.1.6",
    "@sentry/react": "^10.38.0",
    "@sentry/tracing": "^7.120.4",
    "@tailwindcss/forms": "^0.5.11",
    "@tailwindcss/typography": "^0.5.19",
    "@tanstack/react-query": "^5.90.20",
    "@tanstack/react-query-devtools": "^5.91.3",
    "axios": "^1.13.4",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.3",
    "compression": "^1.8.1",
    "cors": "^2.8.6",
    "crypto-js": "^4.2.0",
    "date-fns": "^4.1.0",
    "dompurify": "^3.3.1",
    "dotenv": "^17.2.4",
    "express": "^5.2.1",
    "framer-motion": "^12.33.0",
    "google-auth-library": "^10.5.0",
    "google-spreadsheet": "^5.0.2",
    "googleapis": "^171.4.0",
    "handlebars": "^4.7.8",
    "helmet": "^8.1.0",
    "hpp": "^0.2.3",
    "i18next": "^25.8.4",
    "i18next-browser-languagedetector": "^8.2.0",
    "jsdom": "^28.0.0",
    "jsonwebtoken": "^9.0.3",
    "leaflet": "^1.9.4",
    "mjml": "^4.18.0",
    "moment": "^2.30.1",
    "moment-timezone": "^0.6.0",
    "morgan": "^1.10.1",
    "node-cron": "^4.2.1",
    "node-fetch": "^3.3.2",
    "node-telegram-bot-api": "^0.67.0",
    "nodemailer": "^8.0.1",
    "pug": "^3.0.3",
    "react": "^19.2.4",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "^19.2.4",
    "react-error-boundary": "^6.1.0",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.71.1",
    "react-hot-toast": "^2.6.0",
    "react-i18next": "^16.5.4",
    "react-leaflet": "^5.0.0",
    "react-query": "^3.39.3",
    "react-router-dom": "^7.13.0",
    "react-scripts": "^0.0.0",
    "socket.io": "^4.8.3",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^13.0.0",
    "web-vitals": "^5.1.0",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/node": "^25.2.1",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "@types/react-router-dom": "^5.3.3",
    "@types/testing-library__jest-dom": "^6.0.0",
    "@types/uuid": "^11.0.0",
    "depcheck": "^1.4.7",
    "eslint": "^10.0.0",
    "eslint-config-react-app": "^7.0.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "serve": "^14.2.5",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "keywords": [
    "logistics",
    "transport",
    "warehouse",
    "react",
    "material-ui",
    "google-sheets",
    "vietnam"
  ],
  "author": "MIA Logistics Team",
  "license": "MIT",
  "homepage": ".",
  "proxy": "http://localhost:5050",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "jest": {
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    }
  }
}
