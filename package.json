        "typescript": "^4.9.5",
        "url": "^0.11.4",
        "util": "^0.12.5",
        "vm-browserify": "^1.1.2",
        "web-vitals": "^5.1.0",
        "webpack-bundle-analyzer": "^4.10.1"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "src/**/*.{js,jsx,ts,tsx}": [
            "eslint --fix",
            "prettier --write"
        ],
        "src/**/*.{json,css,md}": [
            "prettier --write"
        ]
    },
    "keywords": [
        "logistics",
        "transport",
        "warehouse",
        "react",
        "material-ui",
        "google-sheets",
        "vietnam"
    ],
    "author": "MIA Logistics Team",
    "license": "MIT",
    "homepage": "http://.",
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=9.0.0"
    },
    "directories": {
        "doc": "docs"
    }
}
