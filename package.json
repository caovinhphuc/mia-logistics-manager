{
    "name": "mia-logistics-manager",
    "version": "1.0.0",
    "description": "MIA Logistics Manager - Hệ thống quản lý vận chuyển chuyên nghiệp",
    "main": "src/index.js",
    "scripts": {
        "start": "craco start",
        "build": "craco build",
        "test": "craco test",
        "eject": "react-scripts eject",
        "dev": "npm start",
        "test:coverage": "craco test --coverage",
        "backend": "npm run backend",
        "build:prod": "npm run build && npm run optimize",
        "optimize": "npm run compress && npm run analyze",
        "compress": "gzip -9 build/static/js/*.js && gzip -9 build/static/css/*.css",
        "analyze": "npm run build && npx webpack-bundle-analyzer build/static/js/*.js",
        "serve": "serve -s build -l 3000",
        "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
        "lint:fix": "eslint src/ --ext .js,.jsx,.ts,.tsx --fix",
        "format": "prettier --write src/**/*.{js,jsx,ts,tsx,json,css,md}",
        "type-check": "tsc --noEmit",
        "test:watch": "craco test --watch",
        "install:all": "npm install && npm install -g craco && npm install -g jest && npm install -g @testing-library/react && npm install -g @testing-library/jest-dom && npm install -g @testing-library/user-event"
    },
    "dependencies": {
        "@emotion/react": "^11.11.1",
        "@emotion/styled": "^11.11.0",
        "@google-cloud/storage": "^6.12.0",
        "@hookform/resolvers": "^3.3.2",
        "@mui/icons-material": "^5.14.19",
        "@mui/lab": "^5.0.0-alpha.155",
        "@mui/material": "^5.14.20",
        "@mui/x-data-grid": "^6.18.2",
        "@mui/x-date-pickers": "^6.18.2",
        "@tanstack/react-query": "^5.8.4",
        "@tanstack/react-query-devtools": "^5.90.2",
        "axios": "^1.6.2",
        "crypto-js": "^4.2.0",
        "date-fns": "^2.30.0",
        "exceljs": "^4.4.0",
        "formik": "^2.4.6",
        "framer-motion": "^12.23.24",
        "google-auth-library": "^8.9.0",
        "google-spreadsheet": "^5.0.2",
        "googleapis": "^126.0.1",
        "html2canvas": "^1.4.1",
        "i18next": "^23.7.6",
        "i18next-browser-languagedetector": "^7.2.0",
        "js-cookie": "^3.0.5",
        "jspdf": "^3.0.3",
        "leaflet": "^1.9.4",
        "lodash": "^4.17.21",
        "moment": "^2.29.4",
        "moment-timezone": "^0.5.43",
        "numeral": "^2.0.6",
