{
  "name": "mia-logistics-manager",
  "version": "1.0.0",
  "description": "MIA Logistics Manager - Hệ thống quản lý vận chuyển chuyên nghiệp",
  "main": "src/index.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "npm test -- --coverage --watchAll=false",
    "test:watch": "npm test -- --watch",
    "eject": "react-scripts eject",
    "dev": "npm run dev:frontend",
    "dev:frontend": "sh -c 'FRONTEND_PORT=${FRONTEND_PORT:-3000} react-scripts start'",
    "dev:backend": "sh -c 'BACKEND_PORT=${BACKEND_PORT:-5050} cd server && npm start'",
    "build:prod": "npm run build && npm run optimize",
    "optimize": "npm run compress && npm run analyze",
    "compress": "gzip -9 build/static/js/*.js && gzip -9 build/static/css/*.css",
    "analyze": "npm run build && npx webpack-bundle-analyzer build/static/js/*.js",
    "serve": "sh -c '\n      PORT=${PORT:-3000} && serve -s build -l $PORT\n    '",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src/ --ext .js,.jsx,.ts,.tsx --fix",
    "lint:check": "eslint src/ --ext .js,.jsx,.ts,.tsx --max-warnings 0",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,json,css,md}",
    "format:check": "prettier --check src/**/*.{js,jsx,ts,tsx,json,css,md}",
    "type-check": "tsc --noEmit",
    "pre-commit": "npm run lint:check && npm run format:check && npm run type-check",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@aofl/webcomponent-css-loader": "^4.0.0-alpha.42",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@google-cloud/storage": "^7.7.0",
    "@hookform/resolvers": "^3.3.2",
    "@mui/icons-material": "^5.14.19",
    "@mui/lab": "^5.0.0-alpha.155",
    "@mui/material": "^5.14.20",
    "@mui/x-data-grid": "^6.18.2",
    "@mui/x-date-pickers": "^6.18.2",
    "@tanstack/react-query": "^5.8.4",
    "ajv": "^8.17.1",
    "axios": "^1.6.2",
    "crypto-js": "^4.2.0",
    "date-fns": "^2.30.0",
    "exceljs": "^4.4.0",
    "google-auth-library": "^9.15.1",
    "googleapis": "^128.0.0",
    "html2canvas": "^1.4.1",
    "i18next": "^23.7.6",
    "i18next-browser-languagedetector": "^7.2.0",
    "js-cookie": "^3.0.5",
    "jspdf": "^3.0.3",
    "leaflet": "^1.9.4",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "numeral": "^2.0.6",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-csv": "^2.2.2",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-error-boundary": "^4.0.11",
    "react-helmet-async": "^2.0.4",
    "react-hook-form": "^7.48.2",
    "react-hot-toast": "^2.4.1",
    "react-i18next": "^13.5.0",
    "react-intersection-observer": "^9.5.3",
    "react-leaflet": "^4.2.1",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.20.1",
    "react-scripts": "^5.0.1",
    "react-virtualized": "^9.22.5",
    "react-window": "^1.8.8",
    "recharts": "^2.8.0",
    "socket.io-client": "^4.7.4",
    "uuid": "^9.0.1",
    "web-vitals": "^5.1.0",
    "webpack-plugin-import-retry": "^1.0.2",
    "workbox-webpack-plugin": "^7.0.0",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/crypto-js": "^4.2.1",
    "@types/jest": "^30.0.0",
    "@types/js-cookie": "^3.0.6",
    "@types/leaflet": "^1.9.8",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.10.0",
    "@types/numeral": "^2.0.5",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "@types/react-router-dom": "^5.3.3",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@types/uuid": "^9.0.7",
    "eslint": "^8.55.0",
    "eslint-config-react-app": "^7.0.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.0",
    "serve": "^14.2.1",
    "typescript": "^3.2.1",
    "webpack-bundle-analyzer": "^4.10.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "keywords": [
    "logistics",
    "transport",
    "warehouse",
    "react",
    "material-ui",
    "google-sheets",
    "vietnam"
  ],
  "author": "MIA Logistics Team",
  "license": "MIT",
  "homepage": ".",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
